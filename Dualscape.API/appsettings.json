{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AWS": {
    "Profile": "default",
    "Region": "us-south-1"
  },
  "Kestrel": {

    "Certificates": {
      "Default": {
        "Path": "/etc/letsencrypt/live/dualscape-api.metahelios.com/fullchain.pem",
        "KeyPath": "/etc/letsencrypt/live/dualscape-api.metahelios.com/privkey.pem"
      }
    },
    "Endpoints": {
      "Http": {
        "Url": "http://dualscape.metahelios.com:80"
      },
      "Https": {
        "Url": "https://dualscape.metahelios.com:443"
      }
    }
  }
}
